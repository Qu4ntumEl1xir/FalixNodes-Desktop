{
  "name": "falixnodes",
  "version": "4.51.0-alpha-4",
  "description": "",
  "main": "src/js/electron/index.js",
  "scripts": {
    "start": "electron  .",
    "build": "electron-builder build",
    "make": "electron-forge package --arch=ia32"
  },
  "author": "KorbsStudio",
  "license": "MIT",
  "dependencies": {
    "@types/node-static": "^0.7.7",
    "electron-context-menu": "^3.1.1",
    "electron-log": "^4.4.1",
    "electron-updater": "^4.3.9",
    "glasstron": "^0.1.0"
  },
  "devDependencies": {
    "electron": "^15.0.0",
    "electron-builder": "^22.11.7"
  },
  "build": {
    "publish": {
      "provider": "generic",
      "url": "https://updates.korbsstudio.com/falix-software/",
      "channel": "alpha"
    },
    "win": {
      "target": "nsis",
      "asar": "false"
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "AppImage",
      "maintainer": "package.korbsstudio.falixnodes",
      "category": "Network",
      "vendor": "Korbs Studio",
      "description": "Accessing Falix, all from one place"
    },
    "nsis": {
      "deleteAppDataOnUninstall": true,
      "shortcutName": "Falix Software",
      "displayLanguageSelector": true,
      "oneClick": false,
      "allowElevation": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": false
    }
  }
}
